{
    "name": "tomschlick/laravel-linkable",
    "description": "URL binding for Laravel models",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "laravel/framework": ">=6.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.9",
        "larapack/dd": "^1.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^5.0 || ^6.0",
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Schlick",
            "email": "tom@schlick.email"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "TomSchlick\\Linkable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomSchlick\\Linkable\\Tests\\": "tests/"
        }
    }
}
